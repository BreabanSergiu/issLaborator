<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="domain.Medicament"
           table="Medicament">
        <id name="id" column="id">
            <generator class="increment"/>
        </id>
        <property name="numeMedicament" column="numeMedicament"/>
        <property name="dataExpirarii" column="dataExpirarii"/>
        <property name="dataFabricarii" column="dataFabricarii"/>
        <property name="cantitatePeStoc" column="cantitatePeStoc"/>

    </class>
</hibernate-mapping>
